{% extends 'base.html.twig' %}

{% block title %}UserAddress{% endblock %}

{% block body %}
    <h1>UserAddress</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_address.id }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ user_address.city }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ user_address.address }}</td>
            </tr>
            <tr>
                <th>ZipCode</th>
                <td>{{ user_address.zipCode }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ user_address.country }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ user_address.firstName }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ user_address.lastName }}</td>
            </tr>
            <tr>
                <th>Others</th>
                <td>{{ user_address.others }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_address_index') }}">back to list</a>

    <a href="{{ path('user_address_edit', {'id': user_address.id}) }}">edit</a>

    {{ include('user_address/_delete_form.html.twig') }}
